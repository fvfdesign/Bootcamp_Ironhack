<!-- views/add.erb -->
<!doctype html>
<html>
<head>
  	<title>Blog</title>
</head>
	<body>

	  <h1>Posts</h1>

		<% @posts.each_with_index do |post, index| %>
		  <a href="/post_details/" + <% index.to_s %>><%= post.title %></a>
		<% end %>



		<h1>New Post</h1>
			<a href="/new_post">Create new post</a>




	</body>
</html>
	




